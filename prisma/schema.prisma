// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  token String
}

model Location {
  id   Int    @id @default(autoincrement())
  name String
}

model Bike {
  id           Int    @id @default(autoincrement())
  type         String
  pricePerDay  Int
}